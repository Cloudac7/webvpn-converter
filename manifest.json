{
    "name": "WebVpn-Converter",
    "version": "1.0.0",
    "description": "可以通过WebVPN访问网页",
    "manifest_version": 3,
    "permissions": [
        "contextMenus",
        "tabs",
        "storage"
    ],
    "background": {
        "service_worker": "js/background.js",
        "type": "module"
    },
    "options_page": "options/options.html",
    "action": {
        "default_title": "点击使用WebVPN访问本页",
        "default_icon": {
            "16": "img/icon16.png",
            "32": "img/icon32.png",
            "48": "img/icon48.png",
            "128": "img/icon128.png"
        }
    },
    "icons": {
        "16": "img/icon16.png",
        "32": "img/icon32.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    }
}
	